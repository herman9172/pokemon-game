{
  "name": "backend-starter",
  "version": "0.0.1",
  "description": "Backend Starter",
  "main": "src/index.ts",
  "scripts": {
    "aws-login": "echo 'SSO authentication script missing'",
    "sso-login": "aws sso login --profile dev",
    "db:migrate": "npx ts-node -r tsconfig-paths/register ./scripts/db/migration.config.ts",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "local": "npm run aws-login && sls offline start -v --stage local --region us-east-1 --port 8201",
    "setup": "./scripts/setup.sh && npm install",
    "test": "jest --coverage --detectOpenHandles -c jest.config.js --colors --maxWorkers=4"
  },
  "author": "OMN1",
  "license": "ISC",
  "homepage": "https://github.com/0mn1-Tech/backend-base#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "aws-lambda": "1.0.6",
    "aws-sdk": "2.730.0",
    "axios": "0.20.0",
    "class-validator": "0.12.2",
    "csv-writer": "1.6.0",
    "generate-password": "1.5.1",
    "mandrill-api": "1.0.45",
    "mysql": "2.18.1",
    "redis": "3.0.2",
    "ts-custom-error": "3.1.1",
    "uuid": "3.4.0",
    "winston": "3.3.3",
    "winston-elasticsearch": "0.9.0"
  },
  "engineStrict": true,
  "engines": {
    "node": "^12"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/args": "3.0.0",
    "@types/aws-lambda": "8.10.59",
    "@types/express": "4.17.7",
    "@types/jest": "26.0.9",
    "@types/mysql": "github:types/mysql",
    "@types/node": "12.12.54",
    "@types/redis": "2.8.25",
    "@types/uuid": "3.4.9",
    "args": "5.0.1",
    "axios-mock-adapter": "1.18.2",
    "clean-webpack-plugin": "3.0.0",
    "dotenv": "8.2.0",
    "dotenv-webpack": "1.8.0",
    "husky": "3.1.0",
    "jest": "26.3.0",
    "liquibase": "1.0.1",
    "ora": "5.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "serverless": "1.78.1",
    "serverless-api-gateway-caching": "1.4.1",
    "serverless-api-gateway-throttling": "1.0.1",
    "serverless-domain-manager": "3.3.2",
    "serverless-offline": "5.12.1",
    "serverless-plugin-split-stacks": "1.9.3",
    "serverless-plugin-split-stacks-circular-dependency-fixer": "0.0.1",
    "serverless-plugin-tracing": "2.0.0",
    "serverless-plugin-warmup": "4.9.0",
    "serverless-prune-plugin": "1.4.3",
    "serverless-pseudo-parameters": "2.5.0",
    "serverless-webpack": "5.3.3",
    "ts-jest": "26.1.4",
    "ts-loader": "7.0.5",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.3",
    "typescript": "3.9.7",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "1.7.2"
  },
  "commitlint": {
    "rules": {
      "references-empty": [
        2,
        "never"
      ]
    },
    "parserPreset": {
      "parserOpts": {
        "issuePrefixes": [
          "###-"
        ]
      }
    }
  }
}
